<form method = "post" action = "movies/edit/<%=@user.id%>">
      <%Movie.all.each do |movie|%>
    <input type="checkbox" name="movie_id" id="<%=movie.id%>" value="<%=movie.id%>" <%='checked' if @user.movies.include?(movie) %> ><%=movie.name%></input>
      <%end%>
      <input type = "submit">
</form>
